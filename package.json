{
  "version": "1.2.0",
  "name": "bingo-maker",
  "productName": "Bingo Maker",
  "author": "Franco Cherubini",
  "description": "App to automatically create bingo sheets and auto select values until a sheet WUnqe45whVou4VIsPew37DoXWNrA",
  "private": true,
  "repository": "https://github.com/cherubinifranco/bingomaker",
  "publish": {
    "provider": "github",
    "private": false,
    "owner": "cherubinifranco",
    "repo": "bingo-maker",
    "releaseType": "release"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.bingomaker",
    "artifactName": "BingoMaker-Setup-${version}.${ext}",
    "files": [
      "**/*",
      "build"
    ],
    "win": {
      "icon": "/icon.ico",
      "target": "nsis"
    }
  },
  "main": "public/main.js",
  "homepage": "./",
  "dependencies": {
    "electron-updater": "^6.3.9",
    "html2pdf.js": "v0.9.3",
    "path": "^0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-scripts": "^5.0.1",
    "use-sound": "^5.0.0"
  },
  "devDependencies": {
    "electron": "^26.2.1",
    "electron-builder": "^25.1.8",
    "tailwindcss": "^3.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
